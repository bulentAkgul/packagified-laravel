{
  "name": "bakgul/packagified-laravel",
  "description": "A package to collect the other packages to perform fully.",
  "authors": [
    {
      "name": "Bulent Akgul",
      "email": "bulent.akgul@me.com"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "bakgul/kernel": "0.1.*",
    "bakgul/file-content": "0.1.*",
    "bakgul/laravel-file-creator": "0.1.*",
    "bakgul/laravel-package-generator": "0.1.*",
    "bakgul/laravel-resource-creator": "0.1.*"
  },
  "autoload": {
    "psr-4": {
      "Bakgul\\Packagify\\": "src/",
      "Bakgul\\Packagify\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bakgul\\Packagify\\PackagifyServiceProvider"
      ]
    }
  }
}