{
  "name": "bakgul/packagified-laravel",
  "description": "This is the main package that collects the other packages in this family to perform fully.",
  "authors": [
    {
      "name": "Bulent Akgul",
      "email": "bulent.akgul@me.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0.2",
    "bakgul/kernel": "1.1.*",
    "bakgul/file-content": "1.1.*",
    "bakgul/file-history": "1.1.*",
    "bakgul/command-evaluator": "1.1.*",
    "bakgul/laravel-file-creator": "1.1.*",
    "bakgul/laravel-resource-creator": "1.1.*",
    "bakgul/laravel-code-generator": "1.1.*",
    "bakgul/laravel-package-generator": "1.1.*"
  },
  "autoload": {
    "psr-4": {
      "Bakgul\\Packagify\\": "src/",
      "Bakgul\\Packagify\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bakgul\\Packagify\\PackagifyServiceProvider"
      ]
    }
  }
}